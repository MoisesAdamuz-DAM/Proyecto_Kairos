<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="Kairos.Paginas.AltaPersona"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="Alta Persona">

    <StackLayout Padding="10" Spacing="10">
        <Entry x:Name="txtNombre" Placeholder="Nombre" />
        <Entry x:Name="txtPais" Placeholder="Pais" />
        <Entry x:Name="txtUbicacion" Placeholder="Ubicación" />
        <Entry x:Name="txtNecesidad" Placeholder="Necesidad" />
        <Entry x:Name="txtHistorial" Placeholder="Historial" />
        <Button
            x:Name="btnDatos"
            Clicked="btnDatos_Clicked"
            Text="Crear" />
    </StackLayout>
</ContentPage>